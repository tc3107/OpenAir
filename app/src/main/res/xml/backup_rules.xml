<?xml version="1.0" encoding="utf-8"?><!--
   Sample backup rules file; uncomment and customize as necessary.
   See https://developer.android.com/guide/topics/data/autobackup
   for details.
   Note: This file is ignored for devices older than API 31
   See https://developer.android.com/about/versions/12/backup-restore
-->
<full-backup-content>
    <include domain="file" path="datastore/openair_playlists.preferences_pb" />
    <exclude domain="file" path="datastore/openair_app_state.preferences_pb" />
    <exclude domain="file" path="datastore/openair_cache.preferences_pb" />
    <exclude domain="database" path="openair.db" />
    <exclude domain="database" path="openair.db-wal" />
    <exclude domain="database" path="openair.db-shm" />
</full-backup-content>
